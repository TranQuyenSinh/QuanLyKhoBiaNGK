@model QuanLyKhoBiaNGK.Models.DetailReceived

<tr>
    <td>@Model?.Product?.Name</td>
    <td>@Model?.Unit</td>
    <td>@Model?.Price</td>
    <td>@Model?.Quantity</td>
    <td>@Model?.Amount</td>
    <td class="text-center"><button onclick="removeDetail(this)" type="button"
            class="btn btn-sm btn-danger btnRemoveDetail">Xóa</button></td>
</tr>
